<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.dynasty.zt.test.dao.mapper.TestMapper" >

    <select id="findOne" resultType="TestDO" parameterType="TestDO" >
        select
          real_name as `data`
        from
          sys_user
        where
          user_uuid = '1'
    </select>


	
</mapper>